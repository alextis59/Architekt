{
  "name": "architekt",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "architekt",
      "version": "0.1.0",
      "workspaces": [
        "packages/*"
      ]
    },
    "node_modules/@architekt/backend": {
      "resolved": "packages/backend",
      "link": true
    },
    "node_modules/@architekt/domain": {
      "resolved": "packages/domain",
      "link": true
    },
    "node_modules/@architekt/frontend": {
      "resolved": "packages/frontend",
      "link": true
    },
    "packages/backend": {
      "name": "@architekt/backend",
      "version": "0.1.0",
      "dependencies": {
        "@architekt/domain": "^0.1.0"
      }
    },
    "packages/domain": {
      "name": "@architekt/domain",
      "version": "0.1.0"
    },
    "packages/frontend": {
      "name": "@architekt/frontend",
      "version": "0.1.0"
    }
  }
}
