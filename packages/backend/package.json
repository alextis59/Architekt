{
  "name": "@architekt/backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "npm --workspace @architekt/domain run build && tsx src/index.ts",
    "build": "npm --workspace @architekt/domain run build && tsc -p tsconfig.json",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "npm --workspace @architekt/domain run build && node ../../scripts/run-tsx-tests.mjs src"
  },
  "dependencies": {
    "@architekt/domain": "^0.1.0",
    "express": "^4.19.2",
    "mongodb": "^6.8.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "@types/express": "^4.17.21",
    "@types/supertest": "^2.0.16",
    "supertest": "^6.3.4"
  }
}
